#include <stdio.h>

int main() {
    int i=1,j=7;

    while(j>=5)
    {
        printf("I=%d J=%d\n",i,j);

        j-=1;
    }
    i=3, j=7;
    while(j>=5)
    {
        printf("I=%d J=%d\n",i,j);

        j-=1;
    }
    i=5, j=7;
    while(j>=5)
    {
        printf("I=%d J=%d\n",i,j);

        j-=1;
    }
    i=7;j=7;
    while(j>=5)
    {
        printf("I=%d J=%d\n",i,j);

        j-=1;
    }
    i=9;j=7;
    while(j>=5)
    {
        printf("I=%d J=%d\n",i,j);

        j-=1;
    }

    return 0;
}
