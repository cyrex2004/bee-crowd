#include <stdio.h>
int main()
{
    int N; //lets say N = 400
    scanf("%d",&N);

    int year = N/365; //year = 1
    N = N%365; //remainder = new N = 35
    printf("%d ano(s)\n",year);

    int month = N/30; // months = 35/30 = 1
    N = N%30; // remainder = new N = 5
    printf("%d mes(es)\n",month);

    int days = N;
    printf("%d dia(s)\n",days);

    return 0;
}
