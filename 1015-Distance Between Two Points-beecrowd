#include <stdio.h>
#include <math.h> //this library is required to do square root maths.
int main()
{
    double x1,y1,x2,y2;
    scanf("%lf %lf", &x1,&y1);
    scanf("%lf %lf", &x2,&y2);
    double p1 = (x2-x1)*(x2-x1);
    double p2 = (y2-y1)*(y2-y1);
    double result = sqrt(p1+p2); //sqrt() function is for square root.
    printf("%.4lf\n",result);
    return 0;
}
