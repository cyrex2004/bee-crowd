#include <stdio.h>

int main() {
    float numbers[6];
    int positive_count = 0;


    for (int i = 0; i < 6; i++) {
        scanf("%f", &numbers[i]);
        if (numbers[i] > 0) {
            positive_count++;
        }
    }


    printf("%d valores positivos\n", positive_count);

    return 0;
}
